<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Parking Management System</title>
    <link href="~/Content/logLanding.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="lbody">
        <div class="grabLogin">
            <img src="~/Image/Rectangle 2.png" />
            <h1 id="greet"></h1>
            <h1>Please Log-in</h1>

            <div>
                @RenderBody()
                <footer>
                    <p>&copy; @DateTime.Now.Year - Developed By CODERs</p>
                </footer>
            </div>
        </div>  
    </div>

    @RenderSection("scripts", required: false)
    <script src="~/Scripts/logScript.js"></script>  
</body>
</html>
